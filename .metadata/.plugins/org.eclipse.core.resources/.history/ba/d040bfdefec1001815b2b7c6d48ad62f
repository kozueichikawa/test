package jp.co.internous.action;

import jp.co.internous.action.SmartPhone;

public class Iphone{
		public static void main(String[] args){
		SmartPhone iphone = new SmartPhone();
		iphone.play();
		iphone.stop();
		iphone.next();
		iphone.back();
		iphone.call();
		iphone.mail();
		iphone.photo();
		iphone.internet();
	
		
		}
}
